{
  "name": "nuxt-monaco-editor",
  "author": "e-chan1007",
  "repository": {
    "type": "git",
    "url": "e_chan1007 <echan041007@gmail.com> (https://e-chan.me)"
  },
  "version": "1.0.0-alpha.11",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postpack": "pinst --enable",
    "_postinstall": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue . && remark . -q",
    "prepack": "pinst --disable && nuxt-module-build",
    "release": "standard-version",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "docs:dev": "vuepress dev docs --port 3000",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress serve docs --port 3000"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.8",
    "vite-plugin-static-copy": "^0.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "husky": "^8.0.0",
    "monaco-editor": "^0.34.0",
    "nuxt": "^3.0.0-rc.8",
    "pinst": "^3.0.0",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.1",
    "remark-preset-lint-recommended": "^6.1.2",
    "standard-version": "^9.5.0",
    "vitepress": "^1.0.0-alpha.10",
    "vuepress": "next"
  },
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      [
        "remark-lint-list-item-indent",
        "mixed"
      ]
    ]
  },
  "packageManager": "yarn@3.2.2"
}
