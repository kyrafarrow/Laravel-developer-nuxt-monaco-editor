{
  "name": "nuxt-monaco-editor",
  "author": "e-chan1007",
  "repository": {
    "type": "git",
    "url": "e_chan1007 <echan041007@gmail.com> (https://e-chan.me)"
  },
  "version": "1.0.0-alpha.11",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postpack": "pinst --enable",
    "_postinstall": "husky install",
    "build": "nuxt-module-build",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts,.vue .",
    "prepack": "pinst --disable && nuxt-module-build",
    "release": "standard-version",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "website:dev": "nuxi dev website",
    "website:generate": "nuxi generate website",
    "website:prepare": "nuxt-module-build --stub && nuxi prepare website"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.8",
    "vite-plugin-static-copy": "^0.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "husky": "^8.0.0",
    "monaco-editor": "^0.34.0",
    "nuxt": "^3.0.0-rc.8",
    "pinst": "^3.0.0",
    "sass": "^1.54.5",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "monaco-editor": "*"
  },
  "packageManager": "yarn@3.2.2"
}
